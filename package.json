{
	"name": "study-electron",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"start": "electron .",
		"build": "electron-packager . --out=dist --asar --overwrite --all",
		"build:darwin": "electron-packager . --out=dist --asar --overwrite --platform=darwin --arch=x64",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"devDependencies": {
		"electron-builder": "^20.39.0",
		"electron-packager": "^13.1.1"
	}
}
